pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
         stage('Print Workspace') {
            steps {
                script {
                    sh 'ls -R'
                }
            }
         }
        stage('Build Docker Image') {
            steps {
                script {
                    docker.build('nodejs:tag','./NodeJsApp')
                }
            }
        }
    }
}
